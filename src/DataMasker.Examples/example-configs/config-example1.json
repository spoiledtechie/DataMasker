{
  "dataSource": {
    "type": "SqlServer",
    "config": {
      "connectionString": "Data Source=home;Initial Catalog=MinistryPlatform;Integrated Security=True"
    }
  },
  "dataGeneration": {
    "locale": "en"
  },
  "tables": [
    {
      "name": "dp_Users",
      "primaryKeyColumn": "User_ID",
      "columns": [
        {
          "name": "Display_Name",
          "type": "Bogus",
          "stringFormatPattern": "{{name.lastname}}"
        },
        {
          "name": "User_Email",
          "type": "Bogus",
          "unique": true,
          "stringFormatPattern": "{{internet.email}}"
        }
      ]
    }
  ]
}